Options -Indexes

#ErrorDocument 403 /pics/forward_hack.htm
#ErrorDocument 404 /images/error404.php

# see http://httpd.apache.org/docs/1.3/mod/mod_rewrite.html

#RewriteEngine On

#RewriteBase /pics

#RewriteOptions MaxRedirects=1


#RewriteRule "^(.*)\?get" "$1"
#RewriteRule "^(.*)/\?(.*)type=pic(.*)" "pic.cml?dir=$1&$2$3"
#RewriteRule "^(.*)/\?(.*)" "pics.php?dir=$1&$2"
#RewriteRule "^(.*)\?(.*)" "pics.php?dir=$1&$2"

#RewriteCond %{REQUEST_FILENAME} -d
#RewriteRule "^(.*)/$" "pics.php?dir=$1/" [last,QSA,S=0]

#RewriteCond %{REQUEST_FILENAME} -d
#RewriteRule "^(.*)$" "$1/" [R,last,QSA]

#RewriteCond %{REQUEST_FILENAME} -f
#RewriteRule "^(.*)/([^/]*)$" "pics.php?dir=$1&file=$2" [last,QSA]

#RewriteCond %{REQUEST_FILENAME} !(.*)/pics.php
#RewriteRule "^(.*)$" "pics.php?dir=$1" [last,QSA,S=0]

#RewriteCond %{REQUEST_URI} !(.*)/pics.php
#RewriteRule "^(.*)$" "pics.php?dir=$1" [last,QSA,S=0]
